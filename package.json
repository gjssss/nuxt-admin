{
  "private": true,
  "packageManager": "pnpm@8.9.2",
  "scripts": {
    "build": "nuxi build --dotenv .env.production.local",
    "build:stub": "pnpm -F nuxt-sequelize dev",
    "dev": "nuxi dev",
    "dev:pwa": "VITE_PLUGIN_PWA=true nuxi dev",
    "start": "node .output/server/index.mjs",
    "typecheck": "vue-tsc --noEmit",
    "lint": "eslint .",
    "postinstall": "nuxi prepare && pnpm build:stub",
    "generate": "nuxi generate",
    "start:generate": "npx serve .output/public"
  },
  "dependencies": {
    "jsonwebtoken": "^9.0.2",
    "lodash-es": "^4.17.21",
    "nprogress": "^0.2.0",
    "nuxt-sequelize": "workspace:*",
    "ohash": "^1.1.3",
    "sass": "^1.69.5",
    "sequelize": "^6.33.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.41.0",
    "@element-plus/nuxt": "^1.0.6",
    "@iconify-json/carbon": "^1.1.21",
    "@iconify-json/twemoji": "^1.1.12",
    "@nuxt/devtools": "^1.0.0",
    "@nuxtjs/color-mode": "^3.3.0",
    "@pinia/nuxt": "^0.5.1",
    "@types/jsonwebtoken": "^9.0.4",
    "@types/lodash-es": "^4.17.10",
    "@types/node": "^20.8.9",
    "@types/nprogress": "^0.2.2",
    "@unocss/eslint-config": "^0.57.1",
    "@unocss/nuxt": "^0.57.1",
    "@vite-pwa/nuxt": "^0.1.1",
    "@vueuse/nuxt": "^10.5.0",
    "consola": "^3.2.3",
    "eslint": "^8.52.0",
    "mysql2": "^3.6.2",
    "nuxt": "^3.8.0",
    "nuxt-icon": "^0.6.1",
    "pinia": "^2.1.7",
    "typescript": "^5.2.2",
    "vue-tsc": "^1.8.22"
  }
}
